{
  "name": "test-module",
  "version": "1.0.0",
  "description": "Test module for dependency upgrade testing",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "test": "jest",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts"
  },
  "dependencies": {
    "express": "5.1.0",
    "lodash": "^4.17.21",
    "body-parser": "2.2.0"
  },
  "devDependencies": {
    "@types/express": "5.0.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "22.15.3",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "7.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "engines": {
    "node": "v21.0.0"
  }
}